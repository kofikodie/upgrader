{
  "name": "upgrader",
  "version": "0.0.1",
  "description": "A tool to upgrade your js/node libraries",
  "author": "kody @kofikodie",
  "bin": {
    "upgrader": "./bin/run"
  },
  "homepage": "https://github.com/kofikodie/upgrader",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "kofikodie/upgrader",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.23.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.12",
    "axios": "^1.3.5",
    "inquirer": "^7.2.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.19",
    "@types/chai": "^4.3.4",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.11",
    "@types/shelljs": "^0.8.11",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "nock": "^13.3.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "upgrader",
    "dirname": "upgrader",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "fix:lint": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "generate": "oclif generate command"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/kofikodie/upgrader/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
